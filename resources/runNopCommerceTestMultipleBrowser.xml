<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="NopCommerce">
	
	<parameter name="environment" value="dev"/>
	<test name="Run on FIREFOX">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.nopcommerce.user.Level_04_Multiple_Browser_01_Register" />
		</classes>
	</test>
	
	<parameter name="environment" value="testing"/>
	<test name="Run on CHROME">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.nopcommerce.user.Level_04_Multiple_Browser_01_Register" />
		</classes>
	</test>

	<test name="Run on COC COC">
		<parameter name="browser" value="coccoc" />
		<classes>
			<class name="com.nopcommerce.user.Level_04_Multiple_Browser_01_Register" />
		</classes>
	</test>

	<test name="Run on BRAVE">
		<parameter name="browser" value="brave" />
		<classes>
			<class name="com.nopcommerce.user.Level_04_Multiple_Browser_01_Register" />
		</classes>
	</test>

	<test name="Run on OPERA">
		<parameter name="browser" value="opera" />
		<classes>
			<class name="com.nopcommerce.user.Level_04_Multiple_Browser_01_Register" />
		</classes>
	</test>

</suite> 
